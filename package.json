  "scripts": {
    "dev": "./node_modules/.bin/env-cmd env/development npx webpack-dev-server --config ./webpack.config.js",
    "build": "./node_modules/.bin/env-cmd env/production npx webpack-cli --config ./webpack.config.js",
    "build:clean": "npm run clean:build && npm run clean:report:size && npm run build",
    "clean:build": "npx rimraf ./build/**",
    "clean:storybook": "npx rimraf ./docs/storybook",
    "clean:report:browserstack": "npx rimraf ./reports/browserstack",
    "clean:report:size": "npx rimraf ./reports/bundle-size",
    "clean:report:flow": "npx rimraf ./reports/flow-coverage",
    "storybook": "./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "storybook:build": "npm run clean:storybook && ./node_modules/.bin/build-storybook -c .storybook -o docs/storybook",
    "lint:js": "./node_modules/.bin/eslint ./src",
    "lint:css": "./node_modules/.bin/stylelint ./src/**/*.scss",
    "lint-staged": "./node_modules/.bin/lint-staged",
    "flow": "./node_modules/.bin/flow",
    "flow-coverage": "npm run clean:report:flow && ./node_modules/.bin/flow-coverage-report",
    "test": "npm run test:smoke",
    "test:smoke": "npm run clean:report:browserstack && ./node_modules/.bin/wdio wdio.conf.js --suite smoke",
    "test:unit": "./node_modules/.bin/jest --config=./jest.config.js",
    "test:functional": "npm run clean:report:browserstack && ./node_modules/.bin/wdio wdio.conf.js",
    "precommit": "npm run lint-staged"
  },
